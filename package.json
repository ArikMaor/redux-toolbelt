  "main": "dist/redux-toolbelt.js",
  "scripts": {
    "not-avail-start": "concurrently --kill-others --prefix \"[{name}]\" --names \"WEBPACK,SERVER\" \"npm run build -- --watch\" \"live-server --no-browser --mount=/dist:./dist src/examples\"",
